{
  "source": {
    "include": ["index.js", "README.md"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "private": true,
    "recurse": true,
    "versions": {
      "from": "1.0.0",
      "include": "minor"
    },
    "templateProxy": "./node_modules/minami",
    "template": "./jsdoc",
    "tutorials": "./jsdoc/tutorial"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true,
    "showInheritedInNav": true,
    "default": {
      "useLongnameInNav": false,
      "includeDate": false,
      "layoutFile": "./jsdoc/tmpl/layout.tmpl",
      "staticFiles": {
        "include": [
          "./node_modules/minami/static",
          "./jsdoc/static"
        ]
      }
    }
  }
}